{
  "name": "@youri-kane/js_utils",
  "version": "1.1.3",
  "description": "a few javascript functions used often we don't want to rewrite all the time",
  "type": "module",
  "main": "dist/js_utils.js",
  "module": "./src/index.js",
  "scripts": {
    "build": "node ./node_modules/webpack/bin/webpack.js --config ./webpack.config.js ./src/index.js",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "test": "jest",
    "test-verbose": "jest --coverage --config ./jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payouri/js_utils.git"
  },
  "keywords": [
    "utility",
    "helpful"
  ],
  "author": "Youri Pailloux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/payouri/js_utils/issues"
  },
  "homepage": "https://github.com/payouri/js_utils#readme",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "canvas": "^2.6.1",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "jsdoc": "^3.6.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
